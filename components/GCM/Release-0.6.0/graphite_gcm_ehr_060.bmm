				name = <"start_time">
				type = <"Date_time">
				is_mandatory = <True>
			>
			["end_time"] = (P_BMM_SINGLE_PROPERTY) <
				name = <"end_time">
				type = <"Date_time">
			>
			["participations"] = (P_BMM_CONTAINER_PROPERTY) <
				name = <"participations">
