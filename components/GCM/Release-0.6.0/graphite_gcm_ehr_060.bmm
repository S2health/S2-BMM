			["setting"] = (P_BMM_SINGLE_PROPERTY) <
				name = <"setting">
				type = <"Terminology_term">
				is_mandatory = <True>
			>
			["other_context"] = (P_BMM_SINGLE_PROPERTY) <
				name = <"other_context">
				type = <"Cluster">
			>
		>
